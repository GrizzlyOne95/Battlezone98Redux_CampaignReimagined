//VERTEX 

//GLSLES

vertex_program CR_Untextured_vertexGLSLES glsles
{
	source untextured-vertex.glsles
	syntax glsles
	entry_point vs_main

	default_params
	{
		param_named_auto wvpMat worldviewproj_matrix
		param_named_auto diffuseColor surface_diffuse_colour
	}
}


//GLSL

vertex_program CR_Untextured_vertexGLSL glsl
{
    source untextured-vertex.glsl
	syntax glsl120
    entry_point vs_main
 
    default_params
    {
        param_named_auto wvpMat worldviewproj_matrix
		param_named_auto diffuseColor surface_diffuse_colour
    }
}


//HLSL SHADER MODEL 4

vertex_program CR_Untextured_vertexHLSL4 hlsl
{
    source CR_untextured-sm4.hlsl
    target vs_4_0
    entry_point untextured_vertex
 
    default_params
    {
        param_named_auto wvpMat worldviewproj_matrix
		param_named_auto diffuseColor surface_diffuse_colour
    }
}


//HLSL SHADER MODEL 3

vertex_program CR_Untextured_vertexHLSL hlsl
{
    source CR_untextured.hlsl
    target vs_3_0
    entry_point untextured_vertex
 
    default_params
    {
        param_named_auto wvpMat worldviewproj_matrix
		param_named_auto diffuseColor surface_diffuse_colour
    }
}



//FRAGMENT

//GLSLES

fragment_program CR_Untextured_fragmentGLSLES glsles
{
	source untextured-fragment.glsles
	syntax glsles
	entry_point ps_main

	default_params
	{
		param_named_auto fogColour fog_colour
		param_named_auto fogParams fog_params
	}
}


