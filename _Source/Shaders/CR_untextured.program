//UNIFIED

vertex_program CR_Untextured_vertex unified
{
	delegate CR_Untextured_vertexGLSLES
	delegate CR_Untextured_vertexGLSL
	delegate CR_Untextured_vertexHLSL4
	delegate CR_Untextured_vertexHLSL
}


fragment_program CR_Untextured_fragment unified
{
	delegate CR_Untextured_fragmentGLSLES
	delegate CR_Untextured_fragmentGLSL
	delegate CR_Untextured_fragmentHLSL4
	delegate CR_Untextured_fragmentHLSL
}





















